source /usr/share/powerlevel9k/powerlevel9k.zsh-theme
source /usr/share/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh

export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion

ANDROID_HOME="/home/alexlecco/.android"
ANDROID_SDK="/home/alexlecco/Android/Sdk/"
ANDROID_SDK_ROOT=${ANDROID_SDK}

alias emu="${ANDROID_SDK}/emulator/emulator"
alias avdmanager="${ANDROID_SDK}/tools/bin/avdmanager"
alias sdkmanager="${ANDROID_SDK}/tools/bin/sdkmanager"

export PATH=${PATH}:$ANDROID_SDK/platform-tools:$ANDROID_SDK/tools

neofetch
